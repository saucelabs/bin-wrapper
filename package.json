{
  "name": "@saucelabs/bin-wrapper",
  "version": "0.3.0",
  "description": "Fetches and runs a binary",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rimraf ./build && tsc",
    "lint": "eslint src/ test/",
    "prepare": "husky install",
    "test": "jest --coverage",
    "release": "tsc && release-it --github.release",
    "release:ci": "tsc && npm run release -- --ci --no-git.requireCleanWorkingDir",
    "release:patch": "npm run release -- patch",
    "release:minor": "npm run release -- minor",
    "release:major": "npm run release -- major"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saucelabs/bin-wrapper.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "author": "DevX <devx@saucelabs.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/saucelabs/bin-wrapper/issues"
  },
  "homepage": "https://github.com/saucelabs/bin-wrapper#readme",
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@tsconfig/node16": "^1.0.3",
    "@types/adm-zip": "^0.5.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/tar-stream": "^2.2.2",
    "eslint": "^8.32.0",
    "eslint-config-typescript": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "release-it": "^15.6.0",
    "rimraf": "^4.1.0",
    "spawk": "^1.8.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "axios": "^1.2.2",
    "tar-stream": "^3.0.0"
  }
}
